def decision_tree(age, income):
    if age <= 30:
        if income == "high":
            return "Buy"
        else:
            return "Don't Buy"
    else:
        if income == "low":
            return "Don't Buy"
        else:
            return "Buy"

# Get input from the user
age = int(input("Enter your age: "))
income = input("Enter your income level (low/high): ").lower()

# Predict using decision tree logic
result = decision_tree(age, income)
print("Decision:", result)
